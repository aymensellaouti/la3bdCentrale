{% extends 'base.html.twig' %}

{% block title %}Hello TabController!{% endblock %}

{% block body %}
    <table border = "2">
        {% for case in tableau %}
        <tr >
            <td>{{ case }}</td>
        </tr>
        {% endfor %}
    </table>
    Nombre de valeurs :  {{ tableau|length }}<br>
    Min :  {{ min(tableau) }}<br>
    Max :  {{ max(tableau) }}

{% endblock %}
